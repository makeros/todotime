{
  "name": "Timedoist",
  "productName": "Timedoist",
  "description": "Unofficial extension for todoist the handle task timeboxing.",
  "keywords": [
    "todoist",
    "timeboxing",
    "tasks"
  ],
  "main": "./main.js",
  "version": "0.0.1",
  "author": "arek czogala <mail@arekczogala.pl>",
  "scripts": {
    "start": "electron .",
    "test:unit": "jest ./src",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "frontend:preferences:watch": "chokidar './frontend/src/**/*.elm' -c 'elm make frontend/src/Preferences.elm --output frontend/preferences.js'"
  },
  "dependencies": {},
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "electron": "5.0.0-beta.7",
    "electron-builder": "^22.4.0",
    "elm": "^0.19.1-3",
    "jest": "^25.1.0"
  }
}
